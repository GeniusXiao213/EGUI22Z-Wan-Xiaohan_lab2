@model LibrarySystem.Models.CreateBookList
@{
    Layout = "_Layout2";
}

@{
    ViewBag.Title = "View My Rental";
}

<h2>View My Rental</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Author</th>
            <th>Title</th>
            <th>Publisher</th>
            <th>Publish Date</th>
            <th>Leased Date</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Books)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Author</td>
                <td>@item.Title</td>
                <td>@item.Publisher</td>
                <td>@item.Date</td>
                <td>@item.Leased</td>
            </tr>
        }
    </tbody>
</table>
